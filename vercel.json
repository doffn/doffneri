{
  "builds": [
    {
      "src": "myProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "/api/.*",
      "use": "@vercel/node"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/staticfiles",
      "config": {
        "distDir": "/staticfiles"
      }
    }
  ],
  "routes": [
   {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    { "src": "/(.*)", "dest": "myProject/wsgi.py" }
  ]
}
